{
    "env": {
        "node": true
    },
    "extends": "eslint:recommended",
	"parserOptions": {"ecmaVersion": 6},
	"rules": {
        "indent": 2,
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "no-unused-vars": 1,
        "semi": 0,
        "no-console": 1,
        "no-undef": 0,
        "no-negated-in-lhs": 0,
        "no-mixed-spaces-and-tabs": 1
    }
}
